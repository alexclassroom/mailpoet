{
  "require": {
    "php-stubs/woocommerce-stubs": "10.3.0",
    "phpstan/phpstan": "1.12.32",
    "phpstan/phpstan-doctrine": "1.5.7",
    "phpstan/phpstan-phpunit": "1.4.2",
    "szepeviktor/phpstan-wordpress": "1.3.5"
  },
  "autoload": {
    "psr-4": {
      "MailPoet\\PHPStan\\Extensions\\": "extensions"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "post-install-cmd": [
      "php prefix-phpstan-doctrine.php"
    ],
    "post-update-cmd": [
      "php prefix-phpstan-doctrine.php"
    ]
  }
}
